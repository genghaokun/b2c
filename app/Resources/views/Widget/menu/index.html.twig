<ul class="nav nav-list">
{{dump(data)}}
	{% for sub1 in data %}
	    <li>
	        <div class="nav-item">
	            <a href="{{ path('category', { id : sub1.id }) }}" >{{ sub1.name }}<i class="fa fa-chevron-right"></i></a>
	            <div>
	                <a href="{{ path('category', { id : sub1.__children[0].id }) }}">{{ sub1.__children[0].name }}</a>&nbsp;&nbsp;&nbsp;
	                <a href="{{ path('category', { id : sub1.__children[1].id }) }}">{{ sub1.__children[1].name }}</a>
	            </div>
	        </div>

	        <div class="well dropdown-menu drop-menu-right">
	            {% for sub2 in sub1.__children %}
	                <div><a href="{{ path('category', { id : sub2.id }) }}">{{ sub2.name }}</a></div>
	            {% endfor %}
	        </div>
	    </li>
	{% endfor %}
</ul>